{% extends 'base.html.twig' %}

{% block title %}Bezeichnung{% endblock %}

{% block body %}
    <h1>Bezeichnung</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ bezeichnung.id }}</td>
            </tr>
            <tr>
                <th>Menge</th>
                <td>{{ bezeichnung.menge }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ bezeichnung.description }}</td>
            </tr>
            <tr>
                <th>Einzelpreis</th>
                <td>{{ bezeichnung.einzelpreis }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_bezeichnung_index') }}">back to list</a>

    <a href="{{ path('app_bezeichnung_edit', {'id': bezeichnung.id}) }}">edit</a>

    {{ include('bezeichnung/_delete_form.html.twig') }}
{% endblock %}
